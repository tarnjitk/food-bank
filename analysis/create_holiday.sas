  set holiday1;
  retain religion;
  retain year;
  if prxmatch("/\d\d\d\d.*:/",raw) then do;
    religion = substrn(raw,1,prxmatch("/\d\d\d\d.*:/",raw)-2);
    year = compress(scan (raw,-1),":");
    delete;
  end;
  
  month = substrn(scan(raw,1,"- "),1,3);
  day = scan(raw,2,"- ");
  SAS_start_date = input(cats(day,month,year),date9.);
  holiday = substrn(raw,index(raw,"- ")+2);
  
  if raw ne " ";
  
  drop raw;
